    "@typechain/ethers-v5": "^6.0.4",
    "@types/chai": "^4.2.15",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.35",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.0.32",
    "hardhat": "^2.1.1",
    "hardhat-gas-reporter": "^1.0.4",
    "prettier": "^2.2.1",
    "ts-generator": "^0.1.1",
    "ts-node": "^9.1.1",
    "typechain": "^8.0.0",
    "typescript": "^4.2.3"
  },
